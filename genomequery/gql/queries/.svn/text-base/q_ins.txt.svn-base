include<tables.txt>
genome NA18506;

H1=select * from READS 
where location>0 and mate_loc<0

H2=select create_intervals() from H1 using intervals(location, location+length)


H3=select merge_intervals(interval_count > 20) from H2

out=select * from MAPJOIN H3, H1 using intervals(location, location+length)

print out
